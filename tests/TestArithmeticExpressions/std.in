void main ( void )
{
    int64_t a = 1;
    a = a - 7;
    a = a * 2;
    a = 2 / 3;

    a = 42 % 6;
    a++;
    a--;
    ++a;
    --a;

    a = a & 42;
    a = a | 3;
    a = a ^ 6;
    a = ~a;
    a = a << 2;
    a = a >> 2;

    a = 0;
    a += 2;
    a -= 1;
    a *= 1;
    a /= 1;
    a %= 1;
    a <<= 3;
    a >>= 3;
    a &= 42;
    a |= 12;
    a ^= 12;
}
